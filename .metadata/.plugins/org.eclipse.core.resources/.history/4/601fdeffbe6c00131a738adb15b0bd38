package com.bluetoothexample;

public class btemsdevices {

    
    public static final int BARCODE = 1959;
    public static final int BLOOD_PRESSURE = 1960;
    public static final int HUMAN_THERMOMETER = 1961;
    public static final int ECG_12LEAD_GLOBAL_INSTRUMENTATION = 1962;
    public static final int ECG_3LEAD_GLOBAL_INSTRUMENTATION = 1963;
    public static final int PHILLIPS_12LEAD = 1964;
    public static final int ZOLL_12LEAD = 1965;
    public static final int MEDTRONIC_12LEAD = 1966;
    public static final int GENERAL_RFID = 1967;
    public static final int NONIN_MEDSONSER = 1968;
    public static final int INJECTOR = 1969;
    
    public static final int ECG_DEV = 0x1;
    public static final int THERMOMETER_DEV = 0x2;
    public static final int BLOOD_PRESSURE_DEV = 0x4;
    public static final int BARCODE_READER_DEV = 0x8;
    public static final int OXYGEN_READ_DEV = 0x10;
    public static final int HEARTBEAT_READ_DEV = 0x20;
    public static final int HEARTBEAT_OXYGEN_READ_DEV = (0x020 | 0x10);
    public static final int RFID_DEV = 0x40;
    public static final int INJECTOR_DEV = 0x80;
    
    public static final String DS6878_NAME = "DS6878";
    public static final String MR12A_NAME = "M12R";
    public static final String MR3A_NAME = "MR3A";
    public static final String CS3070_NAME = "CS3070";
    public static final String NONIN_NAME = "Nonin_Medical_Inc";
  //  public static final String INJECTOR_NAME = "SyringePump.com";
    
 //   public static final String INJECTOR_NAME = "Nexus 4";
     public static final String INJECTOR_NAME ="Patrick Brown";
    
    
    public btemsdevices(int did,String dnam,int dt)
 {
	 device_id = did;
	 device_name = dnam;
	 device_type = dt;
 }
 int device_id;
 String device_name;
 int device_type;
}
